    "ali-oss": "^6.23.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "mongoose": "^9.0.2",
    "multer": "^2.0.2",
    "winston": "^3.19.0",
    "ws": "^8.18.3"
